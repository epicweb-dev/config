{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"isolatedModules": true,
		"jsx": "react-jsx",
		"module": "preserve",
		"target": "ES2022",
		"strict": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"noUncheckedIndexedAccess": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true
	}
}
